        #page-content-wrapper-reg.section
          #payment.container
            .pay_head Make Payment
            .row.form_reg
              .col-sm-12.col-xs-12.col-sm-12
                form#form_pay.form-horizontal
                  .form-group(style='margin-bottom: 2.5%')
                    #name_div.col-md-12.col-xs-12.col-sm-12
                      input#name_pay.form-control(type='text', placeholder='NAME')
                      |  
                      span#error_name_pay.error(style='color: #ffffff; margin-top: 2px') * This field is required
                  .form-group(style='margin-bottom: 2.5%')
                    #email_div.col-md-12.col-xs-12.col-sm-12
                      input#email_pay.form-control(type='email', placeholder='E-MAIL')
                    |  
                    span#error_email_pay.error(style='color: #ffffff; margin-top: 2px') * This field is required with vail email
                  .row(style='margin-bottom: 2%')
                    center
                      .col-lg-6.col-xs-12.col-md-6(style='color: #ffffff; text-align: center')
                        h4 Are you a VIT student :
                      .col-lg-6.col-xs-12.col-md-6
                        .input-group(style='width: 30px;')
                          span.input-group-addon(style='background-color: #0093fc; border: 2px solid #7ccaf6; border-radius: 4px;padding-top: 10px;')
                            input#Checkbox_pay(type='checkbox', aria-label='...')
                  .form-group(style='margin-bottom: 2.5%')
                    #reg_no_div.col-md-12.col-xs-12.col-sm-12
                      input#reg_no_pay.form-control(type='text', pattern='^[1-9]{1}[0-9]{1}[A-Za-z]{3}[0-9]{4}$', style='background-color: #0093fc', placeholder='REGISTRATION No.', disabled='')
                    |  
                    span#error_reg_pay.error(style='color: #ffffff; margin-top: 2px') * This field is required with valid No.
                  .form-group(style='margin-bottom: 2.5%')
                    #uni_name_div.col-md-12.col-xs-12.col-sm-12
                      input#uni_name_pay.form-control(type='text', style='background-color: #0093fc', placeholder='UNIVERSITY / INSTITUTION NAME')
                    |  
                    span#error_uni_pay.error(style='color: #ffffff; margin-top: 2px') * This field is required
                  .form-group(style='margin-top: 2vh;')
                    .col-md-4.col-xs-12.col-sm-12(onclick="pay('name_pay', 'email_pay', 'Checkbox_pay', 'reg_no_pay', 'uni_name_pay');")
                      center
                        button#pay.btn.btn-send(type='submit')
                          center Proceed To Pay
        #page-content-wrapper-contact.section
          .container-fluid
            .row
              #map.col-sm-6.col-md-6.col-xs-12.message
              .col-sm-6.col-md-6.col-xs-12.contact
                p(style='opacity: 0.6; font-size: 25px') There are plenty of ways to reach Vellore
                h4
                  b Start planning your trip now!
                .row.form
                  .col-sm-12.col-xs-12.col-sm-12
                    h4(style='opacity: 0.6') Have a doubt? Ask right away
                    form#form.form-horizontal
                      .form-group
                        #name_div.col-md-12.col-xs-12.col-sm-12
                          input#name.form-control(type='text', placeholder='NAME')
                        |  
                        span#error_name_contact.error(style='color: #ffffff; margin-top: 2px') * This field is required with valid email
                      .form-group
                        #email_div.col-md-12.col-xs-12.col-sm-12
                          input#email.form-control(type='email', placeholder='E-MAIL')
                        |  
                        span#error_email_contact.error(style='color: #ffffff; margin-top: 2px') * This field is required
                      .form-group
                        #message_div.col-md-12.col-xs-12.col-sm-12
                          textarea#message.form-control(rows='8', cols='60', placeholder='YOUR MESSAGE', style='resize: none;')
                        |  
                        span#error_text_contact.error(style='color: #ffffff; margin-top: 2px') * This field is required
                      .form-group
                        .col-md-4.col-xs-12.col-sm-12(onclick="contact('name','email','message');")
                          center
                            button.btn.btn-send(type='submit')
                              center Contact Us
